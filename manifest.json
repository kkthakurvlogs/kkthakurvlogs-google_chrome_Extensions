{
  "manifest_version": 2,
  "name": "Social Media Blocker",
  "version": "0.1.1",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["Content.js"]
    }
  ],
  "icons": { "48": "icon.png", "128": "icon.png" },
  "browser_action": {
    "default_popup": "Popup.html",
    "default_title": "Social Media Blocker"
  },
  "permissions": [
    "https://www.youtube.com/*",
    "https://www.facebook.com/*"
  ]
}
